#!/bin/bash

# -------------------- Home Folder Links ---------------------------------------
ln -s "$PWD/.zshenv" "${HOME}/.zshenv"
ln -s "$PWD/.zsh" "$HOME/.zsh"
ln -s "$PWD/nvim" "$HOME/.config/nvim"
ln -s "$PWD/.tmux.conf" "$HOME/.tmux.conf"
ln -s "$PWD/tmux" "$HOME/.tmux"

touch ${HOME}/.hushlogin

